local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Brookhaven Hub by Princelion33#7607", HidePremium = false, SaveConfig = true, ConfigFolder = "Brookhaven Hub"})
OrionLib:MakeNotification({
	Name = "Discord",
	Content = "Made by Princelion33#7607",
	Image = "rbxassetid://4483345998",
	Time = 30
})
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Local Players"
})
Tab:AddSlider({
	Name = "SpeedPower",
	Min = 0,
	Max = 100,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})
Tab:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 100,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})
local Tab = Window:MakeTab({
	Name = "Fun",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = true
})
local Section = Tab:AddSection({
	Name = "Music chat"
})
Tab:AddButton({
	Name = "Thank you U.S.A",
	Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/Princelion33/Thank-you-U.S.A-Roblox-script/main/Script'))()
  	end    
})
Tab:AddButton({
	Name = "Gangsta's Paradise",
	Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/Princelion33/Gangsta-s-Paradise-Roblox-Script/main/Script'))()
  	end    
})
local Section = Tab:AddSection({
	Name = "Chat spoofer"
})
Tab:AddButton({
	Name = "Chat spoofer",
	Callback = function()
        loadstring(game:HttpGet(('https://pastebin.com/raw/djBfk8Li'),true))()
  	end    
})
local Section = Tab:AddSection({
	Name = "Mod menu"
})
Tab:AddButton({
	Name = "Infinite yield plugins",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Infinite-Store/Infinite-Store/main/main.lua"))()
  	end    
})
OrionLib:Init()
Tab1:AddToggle({
    Name = "Toggle",
    Default = true,
    Save = true,
    Flag = "toggle"
})

print(OrionLib.Flags["toggle"].Value)
OrionLib:Destroy()
